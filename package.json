{
  "name": "psip-navigator",
  "version": "1.0.0",
  "description": "PSIP Navigator - AI-powered health insurance assistant",
  "main": "backend_api.py",
  "scripts": {
    "start": "python3 -m uvicorn backend_api:app --host 0.0.0.0 --port $PORT",
    "build": "cd psip-plan-pal && npm run build",
    "dev": "python3 -m uvicorn backend_api:app --host 0.0.0.0 --port 8000 --reload"
  },
  "keywords": [
    "health-insurance",
    "ai",
    "rag",
    "chatbot",
    "psip",
    "healthcare"
  ],
  "author": "Brett Libowitz",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "lovable": {
    "type": "fullstack",
    "backend": {
      "runtime": "python",
      "startCommand": "python3 -m uvicorn backend_api:app --host 0.0.0.0 --port $PORT",
      "buildCommand": "pip install -r requirements.txt"
    },
    "frontend": {
      "directory": "psip-plan-pal",
      "buildCommand": "npm run build",
      "outputDirectory": "dist"
    },
    "environment": {
      "OPENAI_API_KEY": "required",
      "CHROMA_PATH": "/tmp/chroma_db",
      "COLLECTION_NAME": "benefits_documents",
      "EMBEDDING_MODEL": "all-MiniLM-L6-v2",
      "OPENAI_MODEL": "gpt-4o-mini"
    }
  }
}